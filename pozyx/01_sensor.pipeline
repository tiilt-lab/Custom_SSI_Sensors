<?xml version="1.0"?>

<pipeline>

	<register>
		<load name="python"/>		
		<load name="ioput"/>		
	</register>	
	 
	<sensor create="PythonSensor" script="multitag_positioning_new" block="0.1">
		<output channel="position" pin="position"/>		
	</sensor>

	<consumer create="FileWriter" path="position" type="1">
		<input pin="position" frame="10"/>		
	</consumer>
	
</pipeline>
